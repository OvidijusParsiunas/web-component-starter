{
  "name": "web-component-starter-lit",
  "version": "1.0.0",
  "description": "Starter web component project for lit element",
  "main": "./dist/web-component-starter-lit.js",
  "module": "./dist/web-component-starter-lit.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run analyze",
    "build:watch": "tsc --watch",
    "serve": "wds --watch",
    "build:bundle": "npm run clean && npm run build && rollup --config",
    "clean": "rimraf dist test",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OvidijusParsiunas/web-component-starter.git"
  },
  "author": "OvidijusParsiunas",
  "license": "ISO",
  "bugs": {
    "url": "https://github.com/OvidijusParsiunas/web-component-starter/issues"
  },
  "homepage": "https://github.com/OvidijusParsiunas/web-component-starter#readme",
  "keywords": [
    "starter",
    "boilerplate",
    "scaffold",
    "web-component",
    "custom-element",
    "component",
    "element",
    "lit",
    "lit-element"
  ],
  "dependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.8",
    "@microsoft/fast-element": "^1.11.0",
    "lit": "^2.6.1",
    "rimraf": "^4.1.2",
    "rollup-plugin-summary": "^1.4.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@web/dev-server": "^0.1.35",
    "@web/dev-server-legacy": "^1.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-transform-tagged-template": "^0.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "customElements": "custom-elements.json",
  "files": [
    "dist/**/*",
    "custom-elements.json"
  ]
}
